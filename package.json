    }
  ],
  "dependencies": {
    "byline": "5.0.0",
    "chalk": "4.0.0",
    "event-stream": "4.0.1",
    "isbinaryfile": "4.0.6",
    "minimatch": "3.0.4",
    "minimist": "1.2.5",
    "readdirp": "3.4.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.30.0",
    "mocha": "^9.0.2",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.7.0"
  }
