  "version": "0.6.0",
  "author": "John Postlethwait <john.postlethwait@gmail.com>",
  "website": "http://johnpostlethwait.github.io/fixme",
  "contributors": [],
  "keywords": [
    "notes",
    "hack",
    "todo",
    "fixme",
    "annotate",
    "optimize"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/JohnPostlethwait/fixme.git"
  },
  "main": "./index.js",
  "scripts": {
    "test": "mocha",
    "lint": "eslint --cache .",
    "lint-fix": "eslint --cache --fix ."
  },
  "bin": {
